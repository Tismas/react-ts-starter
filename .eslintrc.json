{
  "extends": ["plugin:prettier/recommended", "plugin:@typescript-eslint/recommended", "eslint-config-prettier"],
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
